@model Member
<h2>Oldest member</h2>
<div>
    <form>
        <input type="hidden" asp-for="Id">
        <div class="mb-3">
            <label asp-for="FirstName" class="form-label"></label>
            <input asp-for="FirstName" type="text" class="form-control" disabled>
        </div>
        <div class="mb-3">
            <label asp-for="LastName" class="form-label"></label>
            <input asp-for="LastName" type="text" class="form-control" disabled>
        </div>
        <div class="mb-3">
            <label asp-for="Gender" class="form-label"></label>
            <input asp-for="Gender" type="text" class="form-control" disabled>
        </div>
        <div class="mb-3">
            <label asp-for="Dob" class="form-label"></label>
            <input asp-for="Dob" type="date" class="form-control" disabled>
        </div>
        <div class="mb-3">
            <label asp-for="PhoneNumber" class="form-label">Phone Number</label>
            <input asp-for="PhoneNumber" type="text" class="form-control" disabled>
        </div>
        <div class="mb-3">
            <label asp-for="BirthPlace" class="form-label">Birth Place</label>
            <input asp-for="BirthPlace" type="text" class="form-control" disabled>
        </div>
        <div class="mb-3 form-check">
            <input asp-for="IsGraduated" type="checkbox" class="form-check-input" disabled>
            <label asp-for="IsGraduated" class="form-check-label">Graduated</label>
        </div>
    </form>
</div>
